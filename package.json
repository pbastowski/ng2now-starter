{
  "name": "ng2now-starter",
  "description": "ng2now starter",
  "version": "0.0.2",
  "author": "Paul Bastowski <pbastowski@gmail.com>",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --open --inline --hot",
    "build-prod": "npm rebuild node-sass && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-cf": "cross-env NODE_ENV=production webpack --progress --hide-modules && cp -r ./static ./dist/ && cp -r ./dist/ ./server/public/ && cp ./index.html ./dist && cp ./index.html ./server/public/",
    "push": "npm version patch && npm run build-cf && cd server && cf push",
    "cp-dist": "cp -r ./static ./dist/ && cp -r ./dist/ ./server/public/ && cp ./index.html ./server/public/"
  },
  "dependencies": {
    "angular": "1.6.3",
    "angular-animate": "1.6.3",
    "angular-sanitize": "1.6.3",
    "angular-translate": "2.15.1",
    "angular-ui-bootstrap": "2.5.0",
    "angular-ui-router": "1.0.0-rc.1",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "2.4.1",
    "moment": "2.18.1",
    "ng2now": "0.0.15"
  },
  
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-plugin-istanbul": "4.1.1",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.24.0",
    "cross-env": "3.2.4",
    "css-loader": "0.27.3",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "json-loader": "0.5.4",
    "node-sass": "4.1.1",
    "pug-html-loader": "1.1.1",
    "pug-loader": "2.3.0",
    "sass-loader": "4.1.1",
    "style-loader": "0.16",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.0-dev.20170323",
    "webpack": "2.3.1",
    "webpack-dev-server": "2.4.2"
  }
}
