{
  "name": "ng2now-starter",
  "description": "ng2now starter",
  "version": "0.0.1",
  "author": "Paul Bastowski <pbastowski@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --open --inline --hot",
    "build-prod": "npm rebuild node-sass && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-cf": "cross-env NODE_ENV=production webpack --progress --hide-modules && cp -r ./static ./dist/ && cp -r ./dist/ ./server/public/ && cp ./index.html ./dist && cp ./index.html ./server/public/",
    "push": "npm version patch && npm run build-cf && cd server && cf push",
    "cp-dist": "cp -r ./static ./dist/ && cp -r ./dist/ ./server/public/ && cp ./index.html ./server/public/"
  },
  "dependencies": {
    "angular": "1.6.1",
    "angular-translate": "2.13.1",
    "angular-ui-router": "1.0.0-beta.3",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "2.4.1",
    "moment": "2.17.1",
    "ng2now": "0.0.15"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-register": "^6.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "json-loader": "^0.5.4",
    "node-sass": "^4.1.1",
    "pug-html-loader": "^1.0.9",
    "pug-loader": "^2.3.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
